<p id="notice"><%= notice %></p>

<h1>My Likes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Fruit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_likes.each do |my_like| %>
      <tr>
        <td><%= my_like.name %></td>
        <td><%= my_like.fruit %></td>
        <td><%= link_to 'Show', my_like %></td>
        <td><%= link_to 'Edit', edit_my_like_path(my_like) %></td>
        <td><%= link_to 'Destroy', my_like, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New My Like', new_my_like_path %>
